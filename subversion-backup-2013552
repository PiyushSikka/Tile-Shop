}

function navigation_menu()
{
    echo "<br><br>";
    echo '&nbsp; <a href = "' .page_index. '"> Home</a>'; 
    echo '&nbsp; <a href = "' .page_buying . '"> Buying</a>'; 
    echo '&nbsp; <a href = "' .page_purchases. '"> Purchases</a>'; 
    echo '&nbsp; <a href = "' .page_account. '"> Account</a>';
}
