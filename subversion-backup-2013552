color: black;
margin: 0;
padding: 0;
}
.container
{
	width: 80%;
	margin: auto;
	overflow: hidden;
}

header
{
	/*-color: #35424a;*/
        background-color: #EFDECD;
	padding-top : 30px;
	min-height: 70px;
	border-bottom: 3px solid #835C3B;
	height: 100px;
	background-position: 0px -5px;
	
}

header #branding
{
	float: left;
}

header nav
{
	float: right;
	margin-top: 13px;
}

