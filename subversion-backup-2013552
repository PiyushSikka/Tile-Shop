<?php
#Revision History 

#Dev             Date                  Desc
#PS(2013552)   2020-10-02      Started to build remaining two pages

include 'PHP/function.php'; 

test_header();  # calling single function for same lines

$product_code = $first_name = $last_name = $city = $comments = $price = $quantity = "";
$product_code_err = $first_name_err = $last_name_err = $city_err = $comments_err = $price_err = $quantity_err = "";

if ($_SERVER["REQUEST_METHOD"] == "POST") 
{
  if(empty($_POST["product_Code"]))
  {
      $product_code_err  = "please enter product code field";
  }
  $product_code = test_input($_POST["product_code"]);
  $first_name = test_input($_POST["first_name"]);
  $last_name = test_input($_POST["last_name"]);
  $city = test_input($_POST["city"]);
  $comments = test_input($_POST["comments"]);
  $price = test_input($_POST["price"]);
  $quantity = test_input($_POST["quantity"]);
}
##########################

?>
<?php link_css() ?>
<header>
		<div class="container">
			<div id="branding">
                                <?php  echo '<img src = "' . folder_images . Clogo . '">'; ?>
				<h1> <span class="highlight">Tile Shop</span> Montreal</h1>

			</div> 
<!--	 Navigation bar 	-->
                    <nav>
                        <ul>
                           <li>	<?php  navigation_menu(); ?> </li>   <!--  Using function for navigation 	-->
                        </ul>
                    </nav>

		
		</div>
    
</header>

    <section class="front_page">	
		<div class="container">
                    <form method="POST" action="<?php echo htmlspecialchars($_SERVER["PHP_SELF"]);?>"> 
                           Product code: <input type="text" name="product_code" required><br>
                           First name:   <input type="text" name="first_name" required><br>
                           Last name:    <input type="text" name="last_name" required><br>
                           City:         <input type="text" name="city" required><br>
                           Comments:     <textarea name="comments" rows="10" cols="30"></textarea><br>
                           Price:        <input type="text" name="price" required> <br>
                           Quantity:     <input type="number" name="quantity" required> <br>
                           
                           <p> <input type="submit" value="submit" name="submit">
                               <input type="reset" value="clear" name="reset"> </p> 
                    </form>                            
		</div>
    </section>
<?php 
echo "<h2>Your Input:</h2>";
echo $product_code;
echo "<br>";
echo $first_name;
echo "<br>";
echo $last_name;
echo "<br>";
echo $city;
echo "<br>";
echo $price;
echo "<br>";
echo $comments;
echo "<br>";
echo $quantity;
?>
<?php
echo "<br><br><br>";

#############################
test_footer(); # calling single function for footer




