		<a href="https://plus.google.com/">   <?php echo '<img src = "' . folder_images . Socialg . '">';?>
		<a href="https://www.skype.com/en/">  <?php echo '<img src = "' . folder_images . Socials . '">';?>
            </footer>
         
    <?php
    
    echo "<br>";
                    
    echo "<span class = 'footer'> &copy; Piyush Sikka (2013552) ". date("Y") . "</span>";      
    echo "</body>";
    echo "</html>";   
