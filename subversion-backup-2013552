<header>
		<div class="container">
			<div id="branding">
                                <?php  echo '<img src = "' . folder_images . Clogo . '">'; ?>
				<h1> <span class="highlight">Tile Shop</span> Montreal</h1>

			</div> 
<!--	 Navigation bar 	-->
                    <nav>
                        <ul>
                           <li>	<?php  navigation_menu(); ?> </li>   <!--  Using function for navigation 	-->
                        </ul>
                    </nav>	
		</div>
    
</header>

    <section class="front_page">	
		<div class="container">
                    <span class="red"> * = Required </span><br> <br>
                    <form method="POST" action="buying.php"> 
                       Product code:<span class="red"> * </span><input type="text" name="product_code"><br><div class="red"><?php echo $product_code_err ?> </div>
                       First name:<span class="red"> *   </span>   <input type="text" name="first_name"> <br> <div class="red"><?php echo $first_name_err ?> </div>
                       Last name:<span class="red"> *    </span>   <input type="text" name="last_name" ><br> <div class="red"> <?php echo $last_name_err?></div>
                       City: <span class="red">*         </span>        <input type="text" name="city" ><br><div class="red"> <?php echo $city_err?></div>
                       Comments:<textarea name="comments" rows="10" cols="30"></textarea><br><div class="red"> <?php echo $comments_err?></div>
                       Price: <span class="red">*        </span>       <input type="text" name="price" > <br><div class="red"> <?php echo $price_err?></div>
                       Quantity: <span class="red">*     </span>     <input type="number" name="quantity"> <br><div class="red"> <?php echo $quantity_err?></div>
                           
                           <p> <input type="submit" value="submit" name="submit">
                               <input type="reset" value="clear" name="reset"> </p> 
