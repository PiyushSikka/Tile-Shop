#PS(2013552)   2020-10-14      Started validating my form

define("first_name_maxlength", 20);
define("last_name_maxlength", 20);
define("city_maxlength", 8);
define("comments_maxlength",200);
define("product_code_maxlength",12); 
define("price_max", 10000.00);
define("quantity_max", 99); 
define("taxes", 12.05);  #Defining Constants

include 'PHP/Function.php'; 

test_header("Buying");  # calling single function for same lines

$product_code = $first_name = $last_name = $city = $comments =  "";
$price = ""; 
$quantity = ""; 
$product_code_err = $first_name_err = $last_name_err = $city_err = $comments_err = $price_err = $quantity_err = "";

if(isset($_POST["submit"]))
{
    $product_code =   htmlspecialchars($_POST["product_code"]);
    $first_name   =   htmlspecialchars($_POST["first_name"]);
    $last_name    =   htmlspecialchars($_POST["last_name"]);
    $city         =   htmlspecialchars($_POST["city"]);
    $comments     =   htmlspecialchars($_POST["comments"]);
    $price        =   htmlspecialchars($_POST["price"]);
    $quantity     =   htmlspecialchars($_POST["quantity"]);
    
    if(mb_strlen($product_code) >  product_code_maxlength)
        {
            $product_code_err=  "The Product code cannot contain more than " .product_code_maxlength. " characters";
        } 
    else
        if(mb_strlen($product_code) == 0)
        {
            $product_code_err = "The Product code cannot be empty";
        }
    else if(stripos($product_code,"p") === false || stripos($product_code, "p")!=0) #if the product code does not start with p or P
        {
            $product_code_err = "Product code must begin with p or P";
        }
           
    if(mb_strlen($first_name) >  first_name_maxlength) #validation of First name
        {
            $first_name_err =  "The Firstname cannot contain more than " .first_name_maxlength . " characters";
        } 
    else
    {
        if(mb_strlen($first_name)  == 0) #validation of First name
        {
            $first_name_err = "The Firstname cannot contain more than " .first_name_maxlength . " characters";
        } 
    }
    
    if(mb_strlen($last_name) >  last_name_maxlength) #validation of Last name
        {
            $last_name_err = "The Lastname cannot contain more than " .last_name_maxlength . " characters";
        } 
    else
    {
        if(mb_strlen($last_name) == 0) #validation of Last name
        {
            $last_name_err = "The Last name cannot be empty";
        } 
    }
    
    if(mb_strlen($city) >  city_maxlength) #validation of city field
        {
            $city_err = "The City cannot contain more than " .city_maxlength . " characters";
        } 
    else
    {
        if(mb_strlen($city) == 0) #validation of city field
        {
            $city_err = "The City cannot be empty ";
        } 
    }
    
    if(mb_strlen($comments) >  comments_maxlength) #validation of comments field
        {
            $comments_err = "The Comments cannot contain more than " .comments_maxlength. " characters";
        } 
        
    if(is_numeric($price))
    {
        if($price > price_max || $price <= 0) #validation of price
        {
            $price_err =  "Price cannot be less than 0 and above than 10000$";
        }
    }
    else
    {
        $price_err = "Please enter Numeric Values";
    }
    
    if(is_numeric($quantity))
    {
        if($quantity > quantity_max || $quantity < 1) #validation Quantity 
        {
            $quantity_err =  "Quantity Should be between 1 and 99";
        }
    }
    else
    {
        $quantity_err = "Please enter Numeric Values";
    }
    if($product_code_err=="" && $first_name_err=="" && $last_name_err=="" && $city_err=="" && $comments_err=="" && $price_err=="" && $quantity_err=="")
    {
        $subtotal = (int)$price * (int)$quantity;
        $tax = $subtotal * taxes / 100;
          $grandtotal = $subtotal + $tax;
          $result = sprintf("%.2f", $grandtotal);    
          $purchases =  array($product_code , $first_name , $last_name , $city , $comments , $price , $quantity , $subtotal , $tax , $result);  
          $myfile = fopen("purchases.txt", 'a') or die("Unable to open file!");
          $purchases_string = json_encode($purchases);
          fwrite($myfile, $purchases_string. "\r\n");
          fclose($myfile);
          $product_code="";
          $first_name="";
          $last_name="";
          $city="";
          $comments="";
          $price="";
          $quantity="";
    }
 
    }
