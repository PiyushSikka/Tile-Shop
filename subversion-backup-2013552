                                $oArray = json_decode($line_of_text); # decoding  string to array
                                #$parts =  explode(",", $line_of_text);
                                echo "<tr><td> $oArray[0]</td> <td> $oArray[1] </td> <td> $oArray[2] </td>
                                        <td> $oArray[3] </td> <td> $oArray[4] </td> <td> $oArray[5]  <td> $oArray[6] </td>
                                         <td> $$oArray[7] </td> <td> $$oArray[8]</td> <td> $$oArray[9] </td> </tr>";
                            }
                            fclose($myfile);
                            ?>
                        </table>                              
		</div>
    </section>
</div>  

