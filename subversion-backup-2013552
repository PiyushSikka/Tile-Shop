                <?php   
                            
                            $myfile = fopen("purchases.txt", 'r') or die("Unable to open file!");
                        
                            while(!feof($myfile))
                            {
                                $line_of_text = fgets($myfile); # reading file line by line 
                                if(!empty($line_of_text))
                                {
                                    $oArray = json_decode($line_of_text); # decoding  string to array

                                    
                                    #echo "<tr><td> $oArray[0]</td> <td> $oArray[1] </td> <td> $oArray[2] </td>
                                    #        <td> $oArray[3] </td> <td> $oArray[4] </td> <td> $oArray[5]  <td> $oArray[6] </td>
                                    #         <td> $$oArray[7] </td> <td> $$oArray[8]</td> <td> $$oArray[9] </td> </tr>";
                                    echo "<tr>";
                                    for($index =0; $index<sizeof($oArray);$index++)
                                    {
                                        if($index ==7 || $index ==8 || $index == 9)
                                        {
                                            if($index ==7 && isset($_GET["command"]) && htmlspecialchars($_GET["command"]) == "color")
                                            {
                                                if($oArray[$index] < 100)
                                                {
                                                    echo "<td style='color:red;'>$ ".$oArray[$index]."</td>";
                                                }
                                                else if($oArray[$index] >= 100 && $oArray[$index]<=999)
                                                {
                                                    echo "<td style='color:#fed8b1;'>$ ".$oArray[$index]."</td>";
                                                }
                                                else if($oArray[$index] >=   1000)
                                                {
                                                    echo "<td style='color:green;'>$ ".$oArray[$index]."</td>";
                                                }
                                            }
                                            else
