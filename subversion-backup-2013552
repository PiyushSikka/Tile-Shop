            if($customer->Save())
            {
                $firstname = '';
                $lastname = '';
                $address = '';
                $city = '';
                $province = '';
                $postalcode = '';
                $username = '';
                $password = '';
            }
            else
            {
                echo "Data was not Saved due to an Error";
            }
        }
